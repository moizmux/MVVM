            />



    </fragment>


    <fragment
        android:id="@+id/orderfragment"
        android:name="com.example.mvvm.ui.cart.OrderFragment"
        android:label="navigation_order_fragment"
        tools:layout="@layout/fragment_order" >



    </fragment>




